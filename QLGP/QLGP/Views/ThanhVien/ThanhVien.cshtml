
@{
    ViewData["Title"] = "ThanhVien";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    html, body {
    margin: 0px;
    padding: 0px;
    width: 100%;
    height: 100%;
    overflow: hidden;
}

#tree {
    width: 100%;
    height: 100%;
}
.loader {
  border: 16px solid #f3f3f3;
  border-radius: 50%;
  border-top: 16px solid #3498db;
  width: 120px;
  height: 120px;
  -webkit-animation: spin 2s linear infinite; /* Safari */
  animation: spin 2s linear infinite;
  position: fixed;
  top: calc(50% - 60px);
  left: calc(50% - 60px);
}
</style>
<section class="blog blog-3 bg--f8 bor-none">
    <div class="container-fluid">
        <div class="loader"></div>
        <div style="width:100%; height:700px;" id="tree"/>
    </div>
</section>

<script type="text/javascript" src="~/Scripts/VNGP/ThanhVien/vngp_ThanhVien_ThanhVien.js"></script>